Algoritmo "semnome"


Var

 numero, soma : real
 i: inteiro

Inicio
//entrada: 4 números
//processamento: soma dos 4 números
//saída: resultado

para i de 1 ate 4 faca
     escreval ("Digite o", i, "° número")
     leia(numero)
     limpaTela()
     
     soma<- soma + numero
     
fimpara

escreval("O resultado da soma é: ", soma)


Fimalgoritmo